{
  "expo": {
    "name": "CheckPwnedPassword",
    "slug": "CheckPwnedPassword",
    "version": "2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "entryPoint": "./src/App.js",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "2.0",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 3,
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.checkpwnedpassword",
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
